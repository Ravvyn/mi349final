<nav>
    <img src="/images/LynnHogenEschLogo.png" width="55px" height="55px">
    <ul>
    {% for link in navigation %}
      <li>
        {% if page.url == link.url %}
          <strong><a href="{{ link.url | url }}">{{ link.title }}</a></strong>
        {% else %}
          <a href="{{ link.url | url }}">{{ link.title }}</a>
        {% endif %}
      </li>
    {% endfor %}
    </ul>
  </nav>